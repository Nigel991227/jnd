{"remainingRequest":"/Users/xieronghuo/jnd-demo/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/xieronghuo/jnd-demo/src/App.vue?vue&type=style&index=0&lang=css&","dependencies":[{"path":"/Users/xieronghuo/jnd-demo/src/App.vue","mtime":1610622339816},{"path":"/Users/xieronghuo/jnd-demo/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/xieronghuo/jnd-demo/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":499162500000},{"path":"/Users/xieronghuo/jnd-demo/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/Users/xieronghuo/jnd-demo/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/xieronghuo/jnd-demo/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["App.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiJA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"App.vue","sourceRoot":"src","sourcesContent":["<template>\n  <div id=\"app\">\n   \n  <el-button type=\"primary\" @click=\"getYc\">主要按钮</el-button>\n\n    <table v-if=\"randdata\">\n      <tr>\n        <th>期号</th>\n        <th>开奖</th>\n        <th>特码</th>\n        <th>预测</th>\n        <th>对错</th>\n      </tr>\n      <tr>\n        <td>{{ issueS }}</td>\n        <td>{{ timespan }}</td>\n        <td>{{tm[0]}}</td>\n        <td></td>\n      </tr>\n      <tr v-for=\"(item,index) in randdata\" :key=\"item.issue\">\n        <td>{{ item.issue }}</td>\n        <td v-if=\"item.issue\">\n          {{ item.c1 }}+{{ item.c2 }}+{{ item.c3 }}={{ item.c4 }}\n        </td>\n        <td>{{tm[index+1]}}</td>\n        <td>{{ Call.getDX(tm[index+1]) }} {{ Call.getDS(tm[index+1]) }}</td>\n       <!-- <td>{{Call.getCombination(tm[index+1])}}</td> -->\n        <td>{{getRightOrNot(item.c4,tm[index+1])}}</td>\n      </tr>\n    </table>\n\n  </div>\n</template>\n\n<script>\nimport Call from \"./sta/call\";\nimport Moment from \"moment\";\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      kjdata: [],//获取的数据\n      randdata: [],//渲染的数据\n      Call: {},\n      newDate: Moment(),\n      timespan: \"\",//时间\n      tm: [],//预测\n      formulaS: 1,//算法标。默认为1\n      type:1,//预测类型\n    };\n  },\n  beforeCreate() {\n    //组建创建完成，dom还未生成\n  },\n  computed: {\n    //监听数据\n    //过滤或排序后的计算属性函数放这里\n    issueS() {\n      return Number(this.randdata[0].issue) + 1;\n    },\n  },\n  created() {\n    //组建创建后\n    this.getJnd();\n  },\n  mounted() {\n    //渲染后\n    this.Call = Call;\n    setInterval(() => {\n      this.newDate = Moment();\n      let time = parseInt(\n        (Moment(this.kjdata.nowDate).valueOf() +\n          240000 -\n          this.newDate.valueOf()) /\n          1000\n      );\n      if (time > 0) {\n        this.timespan = time;\n      } else {\n        if (Math.abs(time) % 10 === 0) this.getJnd();\n        this.timespan = \"开奖中...\";\n      }\n    }, 1000);\n  },\n\n  methods: {\n    //函数、方法\n    getJnd() {\n      let that = this;\n      this.axios\n        .get(\"http://127.0.0.1:8080/getdata?date\" + Moment().format())\n        .then((res) => {\n          that.kjdata = res.data;\n          that.kjdata.length = 20;\n          that.randdata = Array.from(that.kjdata);\n          this.getYc();\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    getYc() {\n      //算法解析\n      let str = \"1+2+10\";\n      let strarr = str.split('+');\n      let ycjg=0;\n      for (let i = 0; i < 21; i++) {\n        ycjg=0;\n        for (let index = 0; index < strarr.length; index++) {\n          let num=parseInt(strarr[index])+i*4;\n          ycjg=ycjg+parseInt(this.conversion(num));\n        }\n\n        this.tm[i]=this.Call.addZero(ycjg);\n      }\n    },\n    getRightOrNot(yy,nn){\n      let data= this.Call.Contrast(yy,nn,this.type);\n      return data.msg\n    },\n    //算法\n    formula() {\n      //算法1\n      //算法2\n      //算法3\n    },\n    //数值转化\n    conversion(num) {\n      num = parseInt(num);\n      let row = num % 4;\n      if (row === 0) {\n        row = 4;\n      }\n      let col = parseInt(num / 4);\n      if (col === 0) {\n        col = 0;\n      }\n      let str = \"c\" + row;\n      return this.kjdata[col][str];\n    },\n  },\n};\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  color: #2c3e50;\n  display: flex;\n  flex: 1;\n  flex-direction: column;\n  justify-content: center;\n  min-width: 320px;\n}\ntable {\n  text-align: center;\n}\n</style>\n"]}]}