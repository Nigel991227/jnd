A,CAAA,IAAA,IAAA,EAAA,KAAA,CAAA,EAAA,KAAA,CAAA,MAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,QAAA;AACA;AACA,KATA,EASA,IATA,CAAA;AAUA,GAzCA;AA2CA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,MAFA,oBAEA;AACA,UAAA,IAAA,GAAA,IAAA;AACA,WAAA,KAAA,CACA,GADA,CACA,uCAAA,MAAA,GAAA,MAAA,EADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,MAAA,GAAA,GAAA,CAAA,IAAA;AACA,QAAA,IAAA,CAAA,MAAA,CAAA,MAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,QAAA,GAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,MAAA,CAAA;AACA,OANA,EAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OATA;AAUA;AAdA;AA3CA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n     \n\n\n    <table v-if=\"randdata\">\n      <tr>\n        <th>期号</th>\n        <th>开奖</th>\n        <th>特码</th>\n        <th>预测</th>\n        <th>对错</th>\n      </tr>\n      <tr>\n        <td>{{issueS}}</td>\n        <td>{{timespan}}</td>\n        <td></td>\n        <td></td>\n      </tr>\n      <tr v-for=\"item in randdata\" :key=\"item.issue\" >\n        <td >{{item.issue}}</td>\n      \n        <td v-if=\"item.issue\">{{item.c1}}+{{item.c2}}+{{item.c3}}={{item.c4}}</td>\n        <td></td>\n        <td>{{Call.getDX(item.c4)}} {{Call.getDS(item.c4)}}</td>\n        <td></td>\n      </tr>\n    </table>\n\n  </div>\n</template>\n\n<script>\nimport Call from './sta/call';\nimport Moment from 'moment';\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      msg: \"hello vue cli\",\n      kjdata: [],\n      randdata:[],\n      Call:{},\n      newDate:Moment(),\n      timespan:'',\n    };\n  },\n  beforeCreate(){\n    //组建创建完成，dom还未生成\n\n  },\n  computed:{\n    //监听数据\n    //过滤或排序后的计算属性函数放这里\n\n    issueS(){\n      return Number(this.randdata[0].issue)+1;\n    },\n  },\n  created() {\n    //组建创建后\n     this.getJnd();\n  },\n  mounted() {\n    //渲染后\n    this.Call=Call;\n    setInterval(()=>{\n      this.newDate=Moment();\n      let time=parseInt(((Moment(this.kjdata.nowDate).valueOf()+240000)-(this.newDate.valueOf()))/1000);\n      if(time>=0){\n        this.timespan=time;\n      }else{\n        if(Math.abs(time)%10===0) this.getJnd();\n        this.timespan='开奖中...';\n      }\n    },1000)\n  },\n\n  methods: {\n    //函数、方法\n    getJnd() {\n      let that=this;\n      this.axios\n        .get(\"http://127.0.0.1:8080/getdata?date\"+Moment().format())\n        .then((res) => {\n          that.kjdata=res.data;\n          that.kjdata.length=20;\n          that.randdata=Array.from(that.kjdata);\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n};\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  color: #2c3e50;\n  display: flex;\n  flex: 1;\n  flex-direction: column;\n  justify-content: center;\n}\ntable{\n  text-align: center;\n\n}\n</style>\n"],"sourceRoot":"src"}]}